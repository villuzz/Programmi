<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:ui="sap.ui.table" xmlns:f="sap.f"
	xmlns:smartTable="sap.ui.comp.smarttable" controllerName="Z001.ZPS_PSID.controller.ElementView" xmlns:html="http://www.w3.org/1999/xhtml">
	<html:style>
		
		.minpadding {
			padding: 0 0 0 3%;
		}
		
		.minpaddingLast {
			padding: 0 1% 0 1%;
		}
		
		html,
		body {
			height: 100%;
		}
		
		.margintop {
			margin-top: 1.2rem;
		}
		
		.paddingAll {
			padding: 1%;
		}
		
		
		td[id*="ElementView--S"] {
			background-color: #f5f5f5 !important;
		}
		td[id*="ElementView--B"] {
			background-color: #ffdd00 !important;
		}
		td[id*="ElementView--P"] {
			background-color: #0088ff !important;
		}
		td[id*="ElementView--D"] {
			background-color: #ff9900 !important;
		}
		td[id*="ElementView--L"] {
			background-color: #73ff00 !important;
		}
		td[id*="ElementView--Q"] {
			background-color: #ff5100 !important;
		}
		
		.NoMargin {
			margin: 0rem;
		}
				
	 </html:style>
	<f:FlexibleColumnLayout id="fcl" class="ContFlex">
		<f:beginColumnPages>
			<Page id="PageSel">
				<customHeader>
					<Bar class="custom-header header-background">
						<contentLeft>
							<Button type="Default" tooltip="List" text="{i18n>List}" icon="sap-icon://activity-items" press="onListVariant"/>
							<Button type="Default" tooltip="Save" text="{i18n>Save}" icon="sap-icon://save" press="onPressVariant"/>
						</contentLeft>
						<contentMiddle>
						</contentMiddle>
						<contentRight ></contentRight>
					</Bar>
				</customHeader>
				<content>
					
					<Panel expandable="true" expanded="true" headerText="{i18n>Expand}" width="100%" class="sapUiTinyMarginBegin">
						<headerToolbar>
							<Toolbar height="6em">
								<HBox height="6em" width="100%">
									<VBox justifyContent="Center" class="sapUiTinyMarginBegin">
										<Label text="{i18n>P_PS_PSID}"/>
										<ComboBox id="Project" items="{/PspidSetSet}" placeholder="{i18n>P_PS_PSID}" width="15em">
											<core:ListItem key="{PsPspid}" text="{PsPspid} - {PsPost1}" /></ComboBox>
									</VBox>
								</HBox>
							</Toolbar>
						</headerToolbar>
						<VBox height="78%" width="100%">
						<HBox height="7em" width="100%">
							<VBox justifyContent="Center" class="sapUiTinyMarginBegin">
								<Label text="{i18n>P_PROJ_STSMA}"/>
								<ComboBox id="StProject" items="{/StProjectSet}" showSecondaryValues="true" placeholder="{i18n>P_PROJ_STSMA}">
									<core:ListItem key="{JStsma}" text="{JStsma}" additionalText="{Txt30}"/>
								</ComboBox>
							</VBox>
							<VBox justifyContent="Center" class="sapUiTinyMarginBegin">
								<Label text="{i18n>P_QUOT_STSMA}"/>
								<ComboBox id="StQuotation" items="{/StQuotationSet}" showSecondaryValues="true" placeholder="{i18n>P_QUOT_STSMA}"><core:ListItem key="{JStsma}" text="{JStsma}" additionalText="{Txt30}"/></ComboBox>
							</VBox>
						</HBox>
						<HBox height="7em" width="100%">
							<VBox justifyContent="Center" class="sapUiTinyMarginBegin">
								<Label text="{i18n>S_QUOT_AUART}"/>
								<MultiComboBox id="DcQuotation" items="{/DcQuotationSet}" showSecondaryValues="true" placeholder="{i18n>S_QUOT_AUART}"><core:ListItem key="{Auart}" text="{Auart}" additionalText="{Bezei}"/></MultiComboBox>
							</VBox>
							<VBox justifyContent="Center" class="sapUiTinyMarginBegin">
								<Label text="{i18n>S_BIL_AUART}"/>
								<MultiComboBox id="BillingType" items="{/BillingTypeSet}" showSecondaryValues="true" placeholder="{i18n>S_BIL_AUART}"><core:ListItem key="{Auart}" text="{Auart}" additionalText="{Bezei}"/></MultiComboBox>
							</VBox>
						</HBox>
						<HBox height="7em" width="100%">
							<VBox justifyContent="Center" class="sapUiTinyMarginBegin">
								<Label text="{i18n>S_LOG_AUART}"/>
								<MultiComboBox id="Logistic" items="{/LogisticSet}" showSecondaryValues="true" placeholder="{i18n>S_LOG_AUART}"><core:ListItem key="{Auart}" text="{Auart}" additionalText="{Bezei}"/></MultiComboBox>
							</VBox>
							<VBox justifyContent="Center" class="sapUiTinyMarginBegin">
								<Label text="{i18n>P_C_KSCHL}"/>
								<ComboBox id="CostConditionType" items="{/CostConditionTypeSet}" showSecondaryValues="true" placeholder="{i18n>P_C_KSCHL}"><core:ListItem key="{Kschl}" text="{Kschl}" additionalText="{Vtext}"/></ComboBox>
							</VBox>
						</HBox>
						<HBox height="7em" width="100%">
							<VBox justifyContent="Center" class="sapUiTinyMarginBegin">
								<Label text="{i18n>P_TM_KSCHL}"/>
								<ComboBox id="TargetMarginCondition" items="{/TargetMarginConditionSet}" placeholder="{i18n>P_TM_KSCHL}" showSecondaryValues="true"><core:ListItem key="{Kschl}" text="{Kschl}" additionalText="{Vtext}"/></ComboBox>
							</VBox>
							<VBox justifyContent="Center" class="sapUiTinyMarginBegin">
								<Label text="{i18n>P_AM_KSCHL}"/>
								<ComboBox id="ActualMarginCondition" items="{/ActualMarginConditionSet}" placeholder="{i18n>P_AM_KSCHL}" showSecondaryValues="true"><core:ListItem key="{Kschl}" text="{Kschl}" additionalText="{Vtext}"/></ComboBox>
							</VBox>
							<VBox justifyContent="Center" class="sapUiTinyMarginBeginLast"></VBox>
						</HBox>
							</VBox>
					</Panel>
				
				</content>
				<footer>
					<Toolbar>
						<ToolbarSpacer/>
						<Button type="Default" tooltip="Search" text="{i18n>Search}" icon="sap-icon://search" press="onSearch"/>
					</Toolbar>
				</footer>
			</Page>
		</f:beginColumnPages>
		<f:midColumnPages>
			<Page id="Page1" enableScrolling="false">
				<customHeader>
					<Bar class="custom-header header-background">
						<contentLeft>
							<Button id="openMenu" text="Menu" press="handlePressOpenMenu" icon="sap-icon://menu2"/>
						</contentLeft>
						<contentMiddle>
							<Image src="./resource/img/epta_logo.png" densityAware="false" width="2.2rem"/>
						</contentMiddle>
						<contentRight >
							<ToggleButton id="BtFullBanc" icon="sap-icon://full-screen" press="onPressFullBanc"/>
							<Button icon="sap-icon://decline" press="onPressDecline"/>
						</contentRight>
					</Bar>
				</customHeader>
				<content>
					<HBox id="Testata" height="13%" width="100%" renderType="Bare" class="White">
						<List id="table" items="{ path: '/ProjectSet', suspended: true , sorter : { path : 'Pspid' } }" width="100%" showNoData="false">
							<items>
								<CustomListItem>
									<Panel expandable="true" headerText="{i18n>Project} {Pspid}" width="auto" expand="onPressExpand" class="sapUiResponsiveMargin">
										<content>
											<HBox height="100%" width="100%" class="paddingAll" renderType="Bare">
												<VBox height="100%" width="50%">
													<HBox class="sapUiTinyMarginTop">
														<Label text="{i18n>Project}" width="11rem"/>
														<core:Icon tooltip="{i18n>Project}" src="sap-icon://group" size="1em"/>
														<Label text="{Pspid}" wrapping="true" class="sapUiTinyMarginBegin"/>
													</HBox>
													<HBox class="sapUiTinyMarginTop">
														<Label text="{i18n>Post1}" width="11rem"/>
														<Label text="{Post1}" wrapping="true"></Label>
													</HBox>
													<HBox class="sapUiTinyMarginTop">
														<Label text="{i18n>Profl}" width="11rem"/>
														<VBox justifyContent="Center">
															<Label text="{Profl}" design="Bold" wrapping="true"/>
															<Label text="{Profltext}" wrapping="true"/>
														</VBox>
													</HBox>
													<HBox class="sapUiTinyMarginTop">
														<Label text="{i18n>Dppprof}" width="11rem"/>
														<VBox justifyContent="Center">
															<Label text="{Dppprof}" design="Bold" wrapping="true"/>
															<Label text="{Dppproftext}" wrapping="true"/>
														</VBox>
													</HBox>
													<HBox class="sapUiTinyMarginTop">
														<Label text="{i18n>Status}" width="11rem"/>
														<VBox justifyContent="Center">
															<Label text="{Status}" design="Bold" wrapping="true"/>
															<Label text="{Statustext}" wrapping="true"/>
														</VBox>
													</HBox>
													<HBox class="sapUiTinyMarginTop">
														<Label text="{i18n>Vkorg}" width="11rem"/>
														<Label text="{Vkorg}" design="Bold" wrapping="true"></Label>
													</HBox>
													<HBox class="sapUiTinyMarginTop">
														<Label text="{i18n>Vtweg}" width="11rem"/>
														<Label text="{Vtweg}" design="Bold" wrapping="true"></Label>
													</HBox>
													<HBox class="sapUiTinyMarginTop">
														<Label text="{i18n>Spart}" width="11rem"/>
														<Label text="{Spart}" design="Bold" wrapping="true"></Label>
													</HBox>
													<HBox class="sapUiTinyMarginTop">
														<Label text="{i18n>Zzvkbur}" width="11rem"/>
														<VBox justifyContent="Center">
															<Label text="{Zzvkbur}" design="Bold" wrapping="true"/>
															<Label text="{Zzvkburtext}" wrapping="true"/>
														</VBox>
													</HBox>
													<HBox class="sapUiTinyMarginTop">
														<Label text="{i18n>Zzvkgrp}" width="11rem"/>
														<VBox justifyContent="Center">
															<Label text="{Zzvkgrp}" design="Bold" wrapping="true"/>
															<Label text="{Zzvkgrptext}" wrapping="true"/>
														</VBox>
													</HBox>
												</VBox>
												<VBox height="100%" width="50%" class="NoMargin">
													<Panel expandable="false" expanded="true" headerText="{i18n>Soldto}" width="auto" class="NoMargin">
														<content>
															<HBox class="sapUiSmallMarginTop">
																<Label text="{Soldto}" wrapping="true"></Label>
															</HBox>
															<HBox class="sapUiTinyMarginTop">
																<Label text="{i18n>Soldtoadd}" width="11rem"/>
																<Label text="{Soldtoadd}" wrapping="true"></Label>
															</HBox>
															<HBox class="sapUiTinyMarginTop">
																<Label text="{i18n>Soldtocountry}" width="11rem"/>
																<Label text="{Soldtocountry}" wrapping="true"></Label>
															</HBox>
														</content>
													</Panel>
													<Panel expandable="false" expanded="true" headerText="{i18n>Shipto}" width="auto" class="NoMargin">
														<content>
															<HBox class="sapUiSmallMarginTop">
																<Label text="{Shipto}" wrapping="true"></Label>
															</HBox>
															<HBox class="sapUiTinyMarginTop">
																<Label text="{i18n>Shiptoadd}" width="11rem"/>
																<Label text="{Shiptoadd}" wrapping="true"></Label>
															</HBox>
															<HBox class="sapUiTinyMarginTop">
																<Label text="{i18n>Shiptocountry}" width="11rem"/>
																<Label text="{Shiptocountry}" wrapping="true"></Label>
															</HBox>
														</content>
													</Panel>
												</VBox>
											</HBox>
										</content>
									</Panel>
								</CustomListItem>
							</items>
						</List>
					</HBox>
					<HBox alignItems="Center" alignContent="Center" height="7%" width="100%" renderType="Bare">
						<Button type="Transparent" class="sapUiTinyMarginBegin" tooltip="{i18n>Quotation}" text="{i18n>Quotation}" icon="sap-icon://approvals"
							press="onPressQuotation" id="btQuotation"/>
						<Button type="Transparent" class="sapUiTinyMarginBegin" tooltip="{i18n>Billing}" text="{i18n>Billing}"
							icon="sap-icon://travel-expense-report" press="onPressBilling" id="btBilling"/>
						<Button type="Transparent" class="sapUiTinyMarginBegin" tooltip="{i18n>Dates}" text="{i18n>Dates}" icon="sap-icon://check-availability"
							press="onPressDates" id="btDates"/>
						<Button type="Transparent" class="sapUiTinyMarginBegin" tooltip="{i18n>Logistic}" text="{i18n>Logistic}" icon="sap-icon://collaborate"
							press="onPressLogistic" id="btLogistic"/>
						<Button type="Transparent" class="sapUiTinyMarginBeginEnd" tooltip="{i18n>Purchasing}" text="{i18n>Purchasing}"
							icon="sap-icon://batch-payments" press="onPressPurchasing" id="btPurchasing"/>
						<Button type="Transparent" id="openMenuElement" icon="sap-icon://grid" text="{i18n>MenuLayout}" press="handlePressOpenMenuLayout"/>
						<Button type="Transparent" icon="sap-icon://expand-all" text="{i18n>ExpandAll}" press="onExpandFirstLevel"/>
						<Button type="Transparent" icon="sap-icon://collapse-all" text="{i18n>CollapseAll}" press="onCollapseAll"/>
					</HBox>
					<HBox id="Posizione" alignItems="Center" height="80%" width="100%" renderType="Bare">
						<Page id="Element1" showHeader="false" class="sapUiNoContentPadding" enableScrolling="true">
							<ui:TreeTable id="tableElement" toggleOpenState="saveToggleState" selectionMode="Single" columnMove="true" visibleRowCountMode="Interactive"
								filter="sofilter" showNoData="false" >
								<ui:columns >
									<ui:Column id="S0" width="17em" showSortMenuEntry="true" sortProperty="WBS_Element" filterProperty="WBS_Element" tooltip="{i18n>WBS_Element}" >
										<Label text="{i18n>WBS_Element}" />
										<ui:template >
											<Button type="Transparent" id="openMenuItems" text="{WBS_Element}" press="handlePressOpenMenuItems"/>
										</ui:template>
									</ui:Column>
									<ui:Column id="S1" width="3em" hAlign="Center" sortProperty="Livello" filterProperty="Livello"  tooltip="{i18n>Stufe}"  filterType="sap.ui.model.type.Integer">
										<Label text="{i18n>Stufe_2}"/>
											<ui:template>
											<Label text="{Livello}" id="StufeID"/>
										</ui:template>
									</ui:Column>
									<ui:Column id="S3" width="13em" sortProperty="Description" filterProperty="Description" tooltip="{i18n>Description}" >
										<Label text="{i18n>Description}" />
										<ui:template>
											<Text text="{Description}" wrapping="false"/>
										</ui:template>
									</ui:Column>
									<ui:Column id="S4" width="3em" hAlign="Center" sortProperty="KDKG1" filterProperty="KDKG1"  tooltip="{i18n>KDKG1}" >
										<Label text="{i18n>KDKG1_2}" />
										<ui:template>
											<Text text="{KDKG1}" wrapping="false"/>
										</ui:template>
									</ui:Column>
									<ui:Column id="S5" width="3em" hAlign="Center" sortProperty="KDKG2" filterProperty="KDKG2" tooltip="{i18n>KDKG2}">
										<Label text="{i18n>KDKG2_2}"  />
										<ui:template>
											<Text text="{KDKG2}"/>
										</ui:template>
									</ui:Column>
									<ui:Column id="S6" width="10em" sortProperty="Project_User_Status" filterProperty="Project_User_Status" tooltip="{i18n>Project_User_Status}" >
										<Label text="{i18n>Project_User_Status}"  />
										<ui:template>
											<Text text="{Project_User_Status}"/>
										</ui:template>
									</ui:Column>
									<ui:Column id="S7" width="10em" sortProperty="Responsible_No" filterProperty="Responsible_No" tooltip="{i18n>Responsible_No}">
										<Label text="{i18n>Responsible_No}"   />
										<ui:template>
											<VBox justifyContent="Center">
												<Label text="{Responsible_No}" design="Bold"/>
												<Label text="{ResponsibleDesc}"/>
											</VBox>
										</ui:template>
									</ui:Column>
									<ui:Column id="B1" visible="{= ${LOCALPARAMS>/Billing} }" width="7em" hAlign="Center" sortProperty="VbelnB" filterProperty="VbelnB"  tooltip="{i18n>VbelnB}">
										<Label text="{i18n>VbelnB}" />
										<ui:template>
											<HBox justifyContent="Center">
												<Button type="Transparent" id="openMenuBilling" text="{ parts: [{path: 'VbelnB'}], formatter: '.LocalFormatter.alphaOutput' }"
													press="handlePressOpenMenuBilling" visible="{ parts:['VbelnB'], formatter: '.LocalFormatter.Visible'}"/>
												<Button type="Reject" text="{i18n>NoOrder}" class="redText" press="handlePressOpenMenuBilling" id="openMenuBillingNo"
													visible="{ parts:[{ path: 'VbelnB'},{ path: 'ServGroup'},{ path: 'Stufe'}], formatter: '.LocalFormatter.BillingVisibleVal'}"></Button>
											</HBox>
										</ui:template>
									</ui:Column>
									<ui:Column id="B2" visible="{= ${LOCALPARAMS>/Billing} }" width="7em" hAlign="Center" sortProperty="PosnrB" filterProperty="PosnrB" tooltip="{i18n>PosnrB}">
										<Label text="{i18n>PosnrB_2}"  />
										<ui:template>
											<HBox width="100%" height="100%" justifyContent="Center">
												<Text text="{ parts: [{path: 'PosnrB'}], formatter: '.LocalFormatter.alphaOutput' }" wrapping="true"/>
											</HBox>
										</ui:template>
									</ui:Column>
									<ui:Column id="B3" visible="{= ${LOCALPARAMS>/Billing} }" width="4em" tooltip="{i18n>FKSAA}" hAlign="Center" sortProperty="FKSAA" filterProperty="FKSAA">
										<Label text="{i18n>FKSAA_2}"   />
										<ui:template>
											<Text text="{FKSAA}" wrapping="true"/>
										</ui:template>
									</ui:Column>
									<ui:Column id="B4" visible="{= ${LOCALPARAMS>/Billing} }" width="4em" tooltip="{i18n>BillingType}" hAlign="Center" sortProperty="BillingTypeB"
										filterProperty="BillingTypeB">
										<Label text="{i18n>BillingType_2}"   />
										<ui:template>
											<Text text="{BillingTypeB}" wrapping="true"/>
										</ui:template>
									</ui:Column>
									<ui:Column id="B5" visible="{= ${LOCALPARAMS>/Billing} }" width="8em" tooltip="{i18n>BMaterial}"  hAlign="Center" sortProperty="Material" filterProperty="Material">
										<Label text="{i18n>BMaterial_2}"  />
										<ui:template>
											<Text text="{Material}" wrapping="true"/>
										</ui:template>
									</ui:Column>
									<ui:Column id="B6" visible="{= ${LOCALPARAMS>/Billing} }" width="10em" tooltip="{i18n>RejReason}" hAlign="Center" sortProperty="RejReason" filterProperty="RejReason">
										<Label text="{i18n>RejReason_2}"   />
										<ui:template>
											<HBox justifyContent="Center">
											<Text text="{RejReason}" visible="{= !${ parts:[{ path: 'RejReason'}], formatter: '.LocalFormatter.ReasonVisibleVal'} }" wrapping="true"/>
											<Button type="Reject" text="{RejReason}" class="redText" press="handlePressOpenMenuBilling"
												visible="{ parts:[{ path: 'RejReason'}], formatter: '.LocalFormatter.ReasonVisibleVal'}"/>
											<Text text="{RejReasonTxt}" wrapping="true" class="sapUiTinyMarginBegin"/>
											</HBox>
										</ui:template>
									</ui:Column>
									<ui:Column id="B7" visible="{= ${LOCALPARAMS>/Billing} }" width="6em" tooltip="{i18n>BillingNetwr}"  hAlign="Center" sortProperty="BillingNetwr" filterProperty="BillingNetwr"
										filterType="sap.ui.model.type.Integer">
										<Label text="{i18n>BillingNetwr_2}"  />
										<ui:template>
											<Text text="{ parts: [{path: 'BillingNetwr'}], formatter: '.LocalFormatter.DeleteZero' }" wrapping="true"/>
										</ui:template>
									</ui:Column>
									<ui:Column id="B8" visible="{= ${LOCALPARAMS>/Billing} }" width="5em"  tooltip="{i18n>BillingWaerk}"  hAlign="Center" sortProperty="BillingWaerk" filterProperty="BillingWaerk" >
										<Label text="{i18n>BillingWaerk}" />
										<ui:template>
											<Text text="{BillingWaerk}" wrapping="true"/>
										</ui:template>
									</ui:Column>
									<ui:Column id="D1" visible="{= ${LOCALPARAMS>/Dates} }" width="11em"  tooltip="{i18n>Estrt}"  hAlign="Center" sortProperty="Estrt" filterProperty="Estrt">
										<Label text="{i18n>Estrt_2}"/>
										<ui:template>
											<HBox justifyContent="Center" >
												<Label text="{ parts:['Estrt'], formatter: '.LocalFormatter.DateSet'}" visible="{= ${NoLogistic} !== 'X' ? true : false }"  />
												<DatePicker id="ForeDate" placeholder="Enter Date" change="onChangeDate"
												value="{Estrt}" valueFormat="yyyyMMdd" displayFormat="medium"
												visible="{= ${NoLogistic} === 'X' ? true : false }" />
												 <Button type="Accept" class="sapUiTinyMarginBegin" icon="sap-icon://accept" press="btnInsertDate" visible="{= ${NoLogistic} === 'X' ? true : false }" />
											</HBox>
										</ui:template>
									</ui:Column>
									<ui:Column id="D2" visible="{= ${LOCALPARAMS>/Dates} }" width="7em" tooltip="{i18n>Eende}"  hAlign="Center" sortProperty="Eende" filterProperty="Eende">
										<Label text="{i18n>Eende_2}" />
										<ui:template>
											<Label text="{ parts:['Eende'], formatter: '.LocalFormatter.DateSet'}"/>
										</ui:template>
									</ui:Column>
									<ui:Column id="D3" visible="{= ${LOCALPARAMS>/Dates} }" width="9em" tooltip="{i18n>ForeDuration}"  hAlign="Center" sortProperty="ForeDuration" filterProperty="ForeDuration">
										<Label text="{i18n>ForeDuration}" />
										<ui:template>
											<HBox justifyContent="Center" >
												<Input id="ForeDuration" change="onChangeDuration" type="Number" value="{ForeDuration}" visible="{= ${NoLogistic} === 'X' ? true : false }" />
												<Label text="{ForeDuration}" visible="{= ${NoLogistic} !== 'X' ? true : false }" />
												<Button type="Accept" class="sapUiTinyMarginBegin" icon="sap-icon://accept" press="btnInsertDuration" visible="{= ${NoLogistic} === 'X' ? true : false }" />
											</HBox>
										</ui:template>
									</ui:Column>
									<ui:Column id="D4" visible="{= ${LOCALPARAMS>/Dates} }" width="7em"  tooltip="{i18n>Pstrt}" hAlign="Center" sortProperty="Pstrt" filterProperty="Pstrt">
										<Label text="{i18n>Pstrt_2}" />
										<ui:template>
											<Label text="{ parts:['Pstrt'], formatter: '.LocalFormatter.DateSet'}"/>
										</ui:template>
									</ui:Column>
									<ui:Column id="D5" visible="{= ${LOCALPARAMS>/Dates} }" width="7em"  tooltip="{i18n>Pende}" hAlign="Center" sortProperty="Pende" filterProperty="Pende">
										<Label text="{i18n>Pende_2}" />
										<ui:template>
											<Label text="{ parts:['Pende'], formatter: '.LocalFormatter.DateSet'}"/>
										</ui:template>
